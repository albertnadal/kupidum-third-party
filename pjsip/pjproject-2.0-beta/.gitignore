# Ignore build directories.
build/

# Ignore ffmpeg objects.
third_party/ffmpeg/**/*.o

# Ignore ffmpeg dependency files.
third_party/ffmpeg/**/*.d

# Ignore ffmpeg pkg-config auto-generated files.
third_party/ffmpeg/**/*.pc

# Ignore ffmpeg archive files.
third_party/ffmpeg/**/*.a

# Ignore ffmpeg specific auto-generated files.
third_party/ffmpeg/.version
third_party/ffmpeg/config.fate
third_party/ffmpeg/config.h
third_party/ffmpeg/config.log
third_party/ffmpeg/config.mak
third_party/ffmpeg/version.h
third_party/ffmpeg/libavutil/avconfig.h

# Ignore third party archive files.
third_party/lib/*

# Ignore pjsip auto-generated binaries.
pjlib-util/bin/*
pjlib-util/lib/*
pjlib/bin/*
pjlib/lib/*
pjmedia/bin/*
pjmedia/lib/*
pjnath/bin/*
pjnath/lib/*
pjsip-apps/bin/*
pjsip/bin/*
pjsip/lib/*

# Ignore pjsip specific auto-generated files.
pjmedia/include/pjmedia-codec/config_auto.h

# Ignore configure generated files.
build.mak
config.log
config.status

# Ignore debug symbols.
*dSYM/
